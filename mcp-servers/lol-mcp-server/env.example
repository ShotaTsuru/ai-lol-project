# League of Legends MCP Server Configuration

# Riot Games API Key (必須)
# https://developer.riotgames.com/ で取得
RIOT_API_KEY=RGAPI-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Default region (オプション)
DEFAULT_REGION=kr

# Cache settings (オプション)
CACHE_TTL=900

# API Rate Limits (オプション)
API_TIMEOUT=10000
MAX_CONCURRENT_REQUESTS=10

# Debug mode (オプション)
DEBUG=false
